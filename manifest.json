{
  "name": "QR creator",
  "description": "Create QR code for current url.",
  "version": "0.1",
  "icons": {
    "16": "icons/qr.png",
    "48": "icons/qr.png",
    "128": "icons/qr.png"
  },
  "permissions": [ "tabs" ],
  "browser_action": {
        "default_icon": "icons/qr.png",
        "default_popup": "index.html"
    },
  "manifest_version": 2
}